<script type="text/javascript">

    $(document).ready(function () {

        // Exibe um modal com o detalhe do controle
        $('.botaoCustomizado').click(function () {

            var idModal = $(this).attr("id").toString().replace("btn", "modal");

            $("#" + idModal).modal();

            return false;

        });

        // Autocomplete
        $('#txtAutocomplete').typeahead({
            ajax: {
                url: "@Url.Action("AutoComplete", "Usuario")",
                timeout: 500,
                triggerLength: 3,
                method: "POST",
                valueField: "UsuarioId",
                displayField: "Nome"
            },
            onSelect: function (item) {
                mostrarNotificacaoSucesso("Autocomplete", "O registro selecionado foi: <ul><li>Valor: " + item.value + "</li><li>Texto: " + item.text + "</li></ul>");
            }
        });

        // Ações
        $('#btnValidarGeral').click(function () {
            $("#frmGeral").submit();
        });

        $('#btnValidarDataHora').click(function () {
            $('#frmDataHora').submit();
        });

        $('#btnValidarOutro').click(function () {
            $("#frmOutro").submit();
        });

        $('#btnValidarDocumento').click(function () {
            $('#frmDocumento').submit();
        });

        $('#btnValidarTelefone').click(function () {
            $('#frmTelefone').submit();
        });

        $('#btnValidarSelect').click(function () {
            $('#frmSelect').submit();
        });

        // Customiza os controles
        carregarControles();

    });

</script>
