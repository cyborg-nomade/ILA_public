@using System.Configuration;


@{
    ViewBag.Title = "Acesso Negado";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<embed id="embedSeguranca" type="text/html" src="/Compartilhado/Seguranca/@(ConfigurationManager.AppSettings["CasisLibrary.CodigoSistema"].ToString())" width="100%" />

@*Java Script - JQuery *@
<script type="text/javascript">

    $(document).ready(function () {

        // Define a altura do embed
        $('#embedSeguranca').height($('#conteudo').height() - 20);

    });

</script>
